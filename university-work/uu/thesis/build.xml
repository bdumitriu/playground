<?xml version="1.0"?> 
<project name="thesis" default="" basedir=".">

	<!-- Run "ant pdf" in all subdirectories -->
	<target name="pdf">
		<subant target="pdf">
			<fileset dir="." includes="*/build.xml" />
		</subant>
	</target>

	<!-- Run "ant clean" in all subdirectories -->
	<target name="clean">
		<subant target="clean">
			<fileset dir="." includes="*/build.xml" />
		</subant>
	</target>

</project>
